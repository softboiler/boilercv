  author = {Glasbey, Chris and {van der Heijden}, Gerie and Toh, Vivian F. K. and Gray, Alision},
  year = {2007},
  journal = {Color Research \& Application},
  volume = {32},
  number = {4},
  pages = {304--309},
  issn = {1520-6378},
  doi = {10.1002/col.20327},
  urldate = {2024-02-15},
  abstract = {We propose a method for identifying a set of colours for displaying 2D and 3D categorical images when the categories are unordered labels. The principle is to find maximally distinct sets of colours. We either generate colours sequentially, to maximize the dissimilarity or distance between a new colour and the set of colours already chosen, or use a simulated annealing algorithm to find a set of colours of specified size. In both cases, we use a Euclidean metric on the perceptual colour space, CIELAB, to specify distances. {\copyright} 2007 Wiley Periodicals, Inc. Col Res Appl, 32, 304--309, 2007},
  copyright = {Copyright {\copyright} 2007 Wiley Periodicals, Inc.},
  langid = {english}
}

@article{alissaExperimentalInvestigationSteam2014,
