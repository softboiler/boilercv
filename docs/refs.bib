@article{akiyamaBubbleCollapseSubcooled1973,
  title = {Bubble Collapse in Subcooled Boiling},
  author = {Akiyama, Mamoru},
  year = {1973},
  journal = {Bulletin of JSME},
  volume = {16},
  number = {93},
  pages = {570--575},
  doi = {10.1299/jsme1958.16.570},
  abstract = {The motion of a vapor bubble in subcooled boiling was studied experimentally using slightly subcooled water, ethanol, and carbon-tetrachloride under the atmospheric pressure. The degree of subcooling ranged usually from about 2{$^\circ$}C to 50{$^\circ$}C, and in a few extreme cases it was extended to below 1{$^\circ$}C. The experimental data for the collapse stage were found to be well correlated by the equations which were derived by assuming a laminar heat trasfrer between the spherical bubble and the surrounding liquid with uniform subcoolings.}
}

